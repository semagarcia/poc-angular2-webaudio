{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "types" : [
        "node",
        "core-js",
        "artyom.js"
    ]/*,
    "typeroots": [
        "./node_modules/@types"
    ]*/
  },
    "exclude": [
        "node_modules"
    ]
}
